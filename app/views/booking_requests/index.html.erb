<h1>Listing booking_requests</h1>

<table>
  <thead>
    <tr>
      <th>Listing</th>
      <th>Requestor first name</th>
      <th>Requestor last name</th>
      <th>Requestor email</th>
      <th>Requested dates</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @booking_requests.each do |booking_request| %>
      <tr>
        <td><%= booking_request.listing %></td>
        <td><%= booking_request.requestor_first_name %></td>
        <td><%= booking_request.requestor_last_name %></td>
        <td><%= booking_request.requestor_email %></td>
        <td><%= booking_request.requested_dates %></td>
        <td><%= link_to 'Show', booking_request %></td>
        <td><%= link_to 'Edit', edit_booking_request_path(booking_request) %></td>
        <td><%= link_to 'Destroy', booking_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booking request', new_booking_request_path %>
